# Grid Trading Bot Environment Variables

# =============================================================================
# Exchange API
# =============================================================================
BINANCE_API_KEY=your_api_key
BINANCE_API_SECRET=your_api_secret

# =============================================================================
# Database (PostgreSQL)
# =============================================================================
DB_HOST=localhost
DB_PORT=5432
DB_NAME=trading_bot
DB_USER=postgres
DB_PASSWORD=your_password

# =============================================================================
# Redis Cache
# =============================================================================
REDIS_HOST=localhost
REDIS_PORT=6379

# =============================================================================
# Notifications
# =============================================================================
DISCORD_WEBHOOK_URL=your_webhook_url

# =============================================================================
# Logging
# =============================================================================
LOG_LEVEL=INFO
